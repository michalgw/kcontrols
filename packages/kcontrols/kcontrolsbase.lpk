<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <Package Version="4">
    <PathDelim Value="\"/>
    <Name Value="KControlsBase"/>
    <Type Value="RunAndDesignTime"/>
    <Author Value="TK (tk@tkweb.eu)"/>
    <CompilerOptions>
