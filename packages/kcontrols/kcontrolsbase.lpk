    <Version Major="1" Minor="7" Release="1"/>
    <Files Count="4">
      <Item1>
        <Filename Value="..\..\source\kfunctions.pas"/>
        <UnitName Value="kfunctions"/>
      </Item1>
      <Item2>
        <Filename Value="..\..\source\kcontrols.inc"/>
        <Type Value="Include"/>
      </Item2>
      <Item3>
        <Filename Value="..\..\source\kxml.pas"/>
        <UnitName Value="kxml"/>
      </Item3>
      <Item4>
        <Filename Value="..\..\source\kres.pas"/>
        <UnitName Value="kres"/>
